{
  "name": "walletbeat",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "ASTRO_TELEMETRY_DISABLED=1 astro",
    "build": "pnpm run astro build",
    "check:all": "pnpm run check:astro && pnpm run check:lint && pnpm run check:syntax && pnpm run check:spelling && pnpm run check:misc",
    "check:precommit": "pnpm run check:lint:quick && pnpm run check:syntax && pnpm run check:spelling && pnpm run check:misc",
    "check:astro": "pnpm run astro check --minimumSeverity=hint --minimumFailingSeverity=hint",
    "check:lint": "eslint --cache src data",
    "check:lint:quick": "WALLETBEAT_PRECOMMIT_FAST=true pnpm run check:lint --report-unused-disable-directives-severity=off",
    "check:spelling": "cspell lint .",
    "check:spelling:quiet": "cspell lint --quiet --no-progress .",
    "check:syntax": "prettier --check .",
    "check:misc": "for script in .husky/checks/*.sh; do echo \"> Running check: $(basename \"${script}\" | cut -d. -f1)...\" >&2; bash \"$script\" || exit \"$?\"; done; echo \"> All checks passed!\" >&2",
    "dev": "pnpm run astro dev",
    "lint": "pnpm run check:syntax --write && pnpm run check:lint --fix && cspell lint .",
    "preview": "pnpm run astro preview",
    "prepare": "test -d .git && pnpm husky || true",
    "fix": "pnpm run lint",
    "ipfs": "deploy/ipfs/ipfs.sh",
    "helios": "deploy/helios/helios.sh",
    "helios:wrap": "deploy/helios/helios-wrap.sh"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.7",
    "@astrojs/sitemap": "^3.3.1",
    "@astrojs/svelte": "^7.0.13",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/js": "^9.27.0",
    "@fontsource/montserrat": "^5.2.5",
    "@material-icons/svg": "^1.0.33",
    "@mui/icons-material": "^6.4.11",
    "@mui/material": "^6.4.11",
    "@mui/system": "^6.4.11",
    "@mui/utils": "^6.4.9",
    "@mui/x-charts": "^7.29.1",
    "@mui/x-data-grid": "^7.29.1",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.6",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "astro": "^5.7.12",
    "autoprefixer": "^10.4.21",
    "classnames": "^2.5.1",
    "cmdk": "^1.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lucide-static": "^0.487.0",
    "marked": "^15.0.12",
    "micromark": "^4.0.2",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^9.1.0",
    "sass-embedded": "^1.87.0",
    "svelte": "^5.28.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.32.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@fleek-platform/cli": "^3.8.3",
    "@kindspells/astro-shield": "^1.7.1",
    "@types/classnames": "^2.3.4",
    "@types/marked": "^6.0.0",
    "blumen": "^0.16.3",
    "cspell": "^8.19.3",
    "eslint": "^9.25.1",
    "eslint-config-love": "^118.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-custom-order": "^2.2.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "orbiter-cli": "^0.8.2",
    "pnpm": "10.4.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-curly": "^0.3.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.4.0+sha512.6b849d0787d97f8f4e1f03a9b8ff8f038e79e153d6f11ae539ae7c435ff9e796df6a862c991502695c7f9e8fac8aeafc1ac5a8dab47e36148d183832d886dd52"
}